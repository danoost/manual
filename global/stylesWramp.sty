\ProvidesPackage{stylesWramp}

%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
%Formatting Definitions
%----------------------

\newcommand{\subscr}[1]{\raisebox{-0.5ex}{\small #1}}  %Subscript
\newcommand{\src}[1]{\texttt{#1}}                      %Source code
\newcommand{\filename}[1]{\emph{#1}}                   %Filenames
\newcommand{\cout}[1]{\{ #1 \}   }                     %Computer output
\newcommand{\program}[1]{\emph{#1}}                    %Program names{eg emacs}
\newcommand{\hardware}[1]{\emph{#1}}                   %Items of hardware

\newcommand{\reg}[1]{\src{\$#1}}                       %Output register form
\newcommand{\BI}[1]{\textbf{\emph{#1}}}                %Bold Italics

%~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
% Predefined Text
%----------------------

%Directives
\newcommand{\text}{\src{.text} }
\newcommand{\data}{\src{.data} }
\newcommand{\bss}{\src{.bss} }
\newcommand{\Global}{\src{.global} }
\newcommand{\ascii}{\src{.ascii} }
\newcommand{\asciiz}{\src{.asciiz} }
\newcommand{\word}{\src{.word} }
\newcommand{\Space}{\src{.space} }
\newcommand{\equ}{\src{.equ} }

\newcommand{\WRAMPmon}{\program{WRAMPmon}}
\newcommand{\wasm}{\program{wasm}}
\newcommand{\wlink}{\program{wlink}}
\newcommand{\trim}{\program{trim}}
\newcommand{\wobj}{\program{wobj}}
\newcommand{\wdis}{\program{wdis}}

